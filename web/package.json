    "axios": "^1.13.2",
    "framer-motion": "^12.29.0",
    "i18next": "^25.8.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "lodash": "^4.17.23",
    "lodash-es": "^4.17.23",
    "react": "^19.2.0",
