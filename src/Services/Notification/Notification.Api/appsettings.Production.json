{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.SignalR": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "HttpPort": 8080
  },
  "Cors": {
    "AllowedOrigins": []
  },
  "Observability": {
    "ServiceName": "NotificationService",
    "Environment": "production",
    "EnableConsoleExporter": false
  }
}
