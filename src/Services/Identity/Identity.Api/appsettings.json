{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.Authentication": "Information",
      "Microsoft.EntityFrameworkCore.Database.Command": "Information",
      "System": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "FrontendUrl": "",
  "Identity": {
    "IssuerUri": "",
    "SecretKey": ""
  },
