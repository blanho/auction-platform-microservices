{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Information"
    }
  },
  "AllowedHosts": "*",
  "Identity": {
    "Authority": ""
  },
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "RabbitMQ": {
    "Host": "",
    "Port": 5672,
    "VirtualHost": "/",
    "Username": "",
    "Password": "",
    "PrefetchCount": 16,
    "ConcurrencyLimit": 10
  },
  "Storage": {
    "Provider": "Local",
    "LocalBasePath": "uploads",
    "TempFolder": "temp",
    "PermanentFolder": "files",
    "MaxFileSize": 10485760,
    "AllowedExtensions": [
      ".jpg",
      ".jpeg",
      ".png",
      ".gif",
      ".pdf",
      ".doc",
      ".docx"
    ],
    "TempFileExpirationHours": 24,
    "BaseUrl": "",
    "AzureBlob": {
      "ConnectionString": "",
      "Container": "uploads"
    }
  },
  "Observability": {
    "ServiceName": "AnalyticsService",
    "ServiceVersion": "1.0.0",
    "Environment": "development",
    "OtlpEndpoint": "http://jaeger:4317",
    "EnableConsoleExporter": false,
    "EnableAspNetCoreInstrumentation": true,
    "EnableHttpClientInstrumentation": true,
    "EnableEFCoreInstrumentation": true,
    "EnableMassTransitInstrumentation": true
  },
  "Audit": {
    "ServiceName": "AnalyticsService",
    "Enabled": true
  }
}
