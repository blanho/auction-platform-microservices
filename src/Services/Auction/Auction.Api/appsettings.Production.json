{
  "Logging": {
    "ServiceName": "AuctionService",
    "Environment": "production",
    "MinimumLevel": "Warning",
    "EnableConsole": true,
    "File": {
      "Enabled": false
    },
    "Elasticsearch": {
      "Enabled": true
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "HttpPort": 8080,
    "GrpcPort": 8081
  },
  "Observability": {
    "ServiceName": "AuctionService",
    "Environment": "production",
    "EnableConsoleExporter": false
  }
}
